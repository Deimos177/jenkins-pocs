/* groovylint-disable-next-line CompileStatic */
pipeline {
    agent any
    stages {
        stage('Clone repo') {
            steps {
                git(
                  credentialsId: 'jenkins-repo',
                  branch: "${params.repoBranch}",
                  url: "${params.repo}",
                )
                sh(script: 'ls')
            }
        }
    }
}
